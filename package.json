{
  "name": "synoem-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "check:biome": "biome check --write .",
    "dev": "turbo dev",
    "dmno": "pnpm exec dmno plugin",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:biome": "biome format --write .",
    "lint": "turbo lint"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@synoem/typescript-config": "workspace:*",
    "turbo": "^2.5.3",
    "typescript": "catalog:",
    "vitest": "^3.1.4"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "react-is": "catalog:"
    }
  }
}
