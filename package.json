{
  "name": "synoem-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check:biome": "biome check --write .",
    "format:biome": "biome format --write .",
    "dmno": "pnpm exec dmno plugin",
    "vault:prod": "pnpm exec dmno plugin -p vault/prod -- setup",
    "vault:prod:add": "pnpm exec dmno plugin -p vault/prod -- add",
    "vault:prod:upsert": "pnpm exec dmno plugin -p vault/prod -- upsert"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@synoem/typescript-config": "workspace:*",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "turbo": "^2.5.3",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@dmno/encrypted-vault-plugin": "^0.0.10",
    "@dmno/netlify-platform": "^0.0.5",
    "@dmno/vercel-platform": "^0.0.3",
    "dmno": "^0.0.39"
  },
  "pnpm": {
    "overrides": {
      "react-is": "catalog:",
      "file-type": "^20.5.0"
    }
  }
}
