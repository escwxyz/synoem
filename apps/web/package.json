{
  "name": "@synoem/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "DMNO_ENV=development dmno run -w -- next dev --port 3001",

    "build:local": "DMNO_ENV=development dmno run -- next build",
    "build:opennext": "opennextjs-cloudflare build",

    "_upload-secrets": "dmno run -- bash ./scripts/upload-secrets.sh",

    "deploy:preview": "DMNO_ENV=preview pnpm run _deploy-cf-opennext",
    "preview": "echo \"Use 'pnpm deploy:preview'. This 'preview' script is for the old opennextjs-cloudflare preview command.\" && DMNO_ENV=preview pnpm run _deploy-cf-opennext-with-live-preview",
    "deploy:prod": "DMNO_ENV=production pnpm run _deploy-cf-opennext",

    "_deploy-cf-opennext": "pnpm run build:opennext && pnpm run _upload-secrets && opennextjs-cloudflare deploy",
    "_deploy-cf-opennext-with-live-preview": "pnpm run build:opennext && pnpm run _upload-secrets && opennextjs-cloudflare preview",

    "cf-typegen": "wrangler types --env-interface CloudflareBindings ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@dmno/nextjs-integration": "^0.0.14",
    "@hookform/resolvers": "^5.0.1",
    "@next-safe-action/adapter-react-hook-form": "^1.0.14",
    "@opennextjs/cloudflare": "^1.0.4",
    "@react-three/drei": "catalog:",
    "@react-three/fiber": "catalog:",
    "@synoem/config": "workspace:*",
    "@synoem/payload": "workspace:*",
    "@synoem/schema": "workspace:*",
    "@synoem/types": "workspace:*",
    "@synoem/ui": "workspace:*",
    "canvas-confetti": "1.9.3",
    "client-only": "^0.0.1",
    "dmno": "^0.0.39",
    "jotai": "^2.12.4",
    "lenis": "^1.3.1",
    "lucide-react": "catalog:",
    "motion": "^12.12.1",
    "next": "catalog:",
    "next-intl": "^4.1.0",
    "next-safe-action": "^7.10.8",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.3",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "catalog:",
    "react-share": "^5.2.2",
    "react-zoom-pan-pinch": "^3.7.0",
    "recharts": "^2.15.3",
    "server-only": "^0.0.1",
    "three": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@lingual/i18n-check": "^0.8.4",
    "@next/bundle-analyzer": "^15.3.2",
    "@synoem/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "wrangler": "^4.16.1"
  }
}
